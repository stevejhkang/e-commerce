<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.shop.DeliveryMapper">

    <select id="selectByUserSn" parameterType="integer" resultMap="DelivertDto">
        select
            *
        from
            shop.deliveries
        where
            user_sn = #{userSn}
    </select>

    <resultMap id="DeliveryDto" type="com.example.shop.dao.delivery.DeliveryDto">
        <association property="user" column="user_sn" select="com.example.shop.UserMapper.selectByUserSn"/>
    </resultMap>


</mapper>